/**********************
*** MAIN NAVIGATION ***
***********************/

.main-nav { 
    list-style: none; 
    width: 100%;
    height: auto;
    padding-left: 0; /* BS3 */
    background-color: #fff;
}
.main-nav li { 
    width: 100%;	
    height: 40px;
    /*	box-shadow: inset 0 1px 0 rgba(255,255,255,0.05), 0 1px 0 rgba(0,0,0,0.2);*/
    border-top: 1px solid #e0e0e0;
    position: relative;
}
.main-nav li:last-child { 
    /*	box-shadow: inset 0 1px 0 rgba(255,255,255,0.05), 0 1px 0 rgba(0,0,0,0.3), 0 2px 0 rgba(255,255,255,0.05); */
}
.main-nav li a {
    position: relative;
    color: #9f9da3; 
    font-size: 14px; 
    text-decoration: none; 
    display: block;
    width: 100%;
    height: 40px;
    padding: 10px 0 10px 40px;
    /*	text-shadow: 0 1px 0 rgba(0,0,0,0.75);*/
}
.main-nav li a:hover {
    background: -webkit-gradient(linear, 0 50%, 0 100%, from(#3193a1), to(#3193a1));
    background: -webkit-linear-gradient(top, #3193a1, #3193a1);
    background: -moz-linear-gradient(top, #3193a1, #3193a1);
    background: -o-linear-gradient(top, #3193a1, #3193a1);
    background: -ms-linear-gradient(top, #3193a1, #3193a1);
    background: linear-gradient(to bottom, #3193a1, #3193a1);
    color:#fff;
}
.main-nav li a i { 
    margin-right: 10px; 
    /*	text-shadow: 0 2px 4px rgba(0,0,0,0.5);*/
    font-size: 16px;
    color: #c2c8d1;
}

.main-nav li a:hover i, .main-nav li.active a i, .main-nav li.collapsible a i  { 
    color: #c2c8d1;
}
.main-nav li.active a {
    background: -webkit-gradient(linear, 0 50%, 0 100%, from(#3193a1), to(#3193a1));
    background: -webkit-linear-gradient(top, #3193a1, #3193a1);
    background: -moz-linear-gradient(top, #3193a1, #3193a1);
    background: -o-linear-gradient(top, #3193a1, #3193a1);
    background: -ms-linear-gradient(top, #3193a1, #3193a1);
    background: linear-gradient(to bottom, #3193a1, #3193a1);
    color: #fff;
    /*	box-shadow: inset 0 1px 0 rgba(255,255,255,0.34);*/
}
.main-nav li.active::after {
    content:'';
    width: 0; height: 0;
    border-left: 8px solid #3193A1;
    border-top: 5px solid transparent;
    border-bottom: 5px solid transparent;
    position: absolute;
    right: -8px;
    top: 50%;
    margin-top: -5px;
    z-index: 99;
}
.main-nav li.collapsible {	height: auto; }
/*.main-nav li.collapsible.open > .sub-menu { max-height: 500px; }*/  
.main-nav li > .sub-menu { max-height: 0; transition: max-height 0.5s ease-in-out; overflow: hidden; padding-left: 0; /* BS3 */}
.main-nav li.collapsible > a::after {
    content: '';
    font-family: FontAwesome;
    font-size: 12px;
    position: absolute;
    right: 25px;
    /*	text-shadow: 0 2px 4px rgba(0,0,0,0.5);*/
    color: #c2c8d1;
}
.main-nav li.collapsible.open > a::after { 
    content: ''; 
    color: #9F9DA3;
}
.main-nav li > a:hover:after {  
    color: #ececec;
}
.main-nav li.collapsible.open > a { color: #9F9DA3; background: #fff; }
.sub-menu li, .sub-menu li:last-child {
    background: #fff;
    /*	box-shadow: 0 -1px 0 rgba(0,0,0,0.3), inset 0 1px 0 rgba(255,255,255,0.05);*/
}
.sub-menu li a { padding: 10px 0 10px 65px;}






