.slider.range-min .range-tooltip { top:-30px; }
.range-tooltip, .progress-tooltip {
    background: #f00;
    width: 50px;
    height: 22px;
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#3193a1), to(#3193a1));
    background: -moz-linear-gradient(top, #3193a1, #3193a1);
    background: -webkit-linear-gradient(top, #3193a1, #3193a1);
    background: -o-linear-gradient(top, #3193a1, #3193a1);
    background: -webkit-linear-gradient(to bottom, #3193a1, #3193a1);
    border: 1px solid #2d2b2e;
    border-radius: 3px;
    box-shadow: inset 0 1px 0 rgba(255,255,255,0.06), 0 1px 0 rgba(0,0,0,0.15);
    position: absolute;
    top: -37px;
    left: 50%;
    margin-left: -25px;
    text-align: center;
    line-height: 22px;
    font-size: 12px;
    color: #fff;
    font-weight: bold;
    text-shadow: 0 1px 0 rgba(0,0,0,0.50);
    font-family: Arial, helvetica, 'sans';
}
.range-tooltip::after{
    content: '';
    width: 0; height: 0;
    position: absolute;
    border-top: 5px solid #3193a1;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    bottom:-5px;
    left:50%;
    margin-left: -5px;
}
.progress-tooltip::after{
    content: '';
    width: 0; height: 0;
    position: absolute;
    border-top: 5px solid #3193a1;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    bottom:-5px;
    left:50%;
    margin-left: -5px;
}
