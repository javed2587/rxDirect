/* LOADING */
#loading {
    background:rgba(0,0,0,0.85);
    width:100%;
    height: 100%;
    position: fixed;
    top: 0;
    z-index: 1000;
}
#loading > div, #loading-example > div {
    width: 60px;
    height: 60px;
    position: absolute;
    left: 50%;
    margin-left: -30px;
    top: 50%;
    margin-top: -30px;
}

#loading-example {
    position: relative;
    height: 100px;
    width: 100px;
    margin: 0 auto;
}

#loading > div > div, #loading-example > div > div {
    content: '';
    position: absolute;
    width: 16px;
    height: 16px;
    background: #8fd7d4;
    top: 10px;
    left: 10px;
    transform-origin: 10px 10px;
    border-radius: 8px;
    animation: spin-a 1s infinite cubic-bezier(0.5, 0, 0.5, 1);
}

#loading > div > div:nth-child(2), #loading-example > div > div:nth-child(2) {	left: 50%;	animation-delay:0.3s; }
