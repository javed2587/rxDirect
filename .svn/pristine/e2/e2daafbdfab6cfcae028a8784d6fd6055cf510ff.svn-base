/*Dependents,History,Detail,Cards Modal Css*/
.tableModal .table-responsive{
    max-height:350px;
}
.tableModal,
.listModal,
.imgModal{
    height:100%;
}
.tableModal.modal .modal-dialog,
.listModal.modal .modal-dialog,
.imgModal.modal .modal-dialog{
    top:65%;
    width:1262px;
    left: 0.5%;
  
    -webkit-transform:translateY(-70%);
    transform:translateY(-70%);
}
.tableModal .close,
.listModal .close,
.imgModal .close{
    color:#fff;
    font-size:24px;
    font-weight:600;
    opacity:1;
    margin:4px 12px 0 0;
}
.tableModal .modal-header,
.listModal .modal-header,
.imgModal .modal-header{
    max-height: 38px;
    padding:0;
    background-color: #2171b6;
}
.tableModal .modal-header h4,
.listModal .modal-header h4,
.imgModal .modal-header h4{
    line-height:38px;
    padding:0 20px;
    color:#fff;
    text-transform: none;
    font-weight:600;
    font-size: 16px;
}
.tableModal .modal-body,
.listModal .modal-body{
    padding:0;
}
.tableModal table{
    margin:0;
}
.tableModal th{
    color:#669900;
    background-color:#dddddd;
    min-height:40px;
}
.tableModal td{
    color:#676767;
    min-height:40px;
}
.tableModal td:first-child,.tableModal th:first-child{
    padding-left:20px;
}
.tableModal tbody > tr:nth-of-type(odd){
    background-color: #f6f6f6;
}
.tableModal tbody > tr:nth-of-type(even){
    background-color: #dddddd;
}
.listModal .modal-body{
    background-color:#dddddd;
    padding-bottom:16px;
}
.listModal .modal-content{
    width: 100%;
}
.listModal ul,
.listModal dl{
    list-style: none;
    margin:0;
}
.listModal ul{
    padding-left:21px;
}
.listModal dt{
    color:#666666;
}
.listModal dd{
    color:#2170b3;
    font-weight: 600;
}
.listModal li{
    width:50%;
    float:left;
    margin:14px 0 0;
}
.listModal li:nth-last-child(3){
    width:100%;
}
.imgModal .modal-body{
    padding:20px 35px;
    position:relative;
}
.imgModal.modal .modal-dialog{
    -webkit-transform:translateY(-50%);
    transform:translateY(-50%);
}
.imgModal .modal-content,
.imgModal.modal .modal-dialog{
    width:565px;
    top: 50% !important;
}
.imgModal img{
    width:487px;
    height:323px;
}
.imgModal i{
    position:absolute;
    top:50%;
    font-size:26px;
    font-weight: 600;
    color:#999999;
    -webkit-transform:translateY(-50%);
    transform:translateY(-50%);
    display:none;
    cursor:pointer;
}
.imgModal i.fa-chevron-right{
    right:8px;
}
.imgModal i.fa-chevron-left{
    left:6px;
}
.insuranceContainer > div > img{
    cursor:pointer;
}
#medicationModalRefill .modal-header h4{
    background:#bf9000;
    padding: 0 10px;
}
#medicationModalRefill #medicationInfoDiv{
    padding-top:10px;
    padding-bottom:10px;
}
.icmedicen label{
    font-size:13px;
    font-weight:600;
    text-transform:uppercase;
    color:#fe1115;	
}
.icmedicen span{
    font-weight:bold;
    color:#000;
}
#medicationModalRefill .refill_medi{
    padding:0 15px;
}
.refill_options{
}
.refill_options span{
    font-weight:600;
    color:#346fc1;
    font-size:14px;
    padding:8px 4px 4px 0;
    display:inline-block;
    vertical-align:top;
}
.refill_options .fill_field{
    border:0;
    outline:0;
    color:#350306;
    padding:8px 9px 4px;
    height:auto;
    font-size:14px;
    border-bottom:#256fbd 1px solid;
    background:none;
    vertical-align:top;
}
.refill_options .fill_field:focus, .refill_options .fill_field:hover{
    border:0;
    outline:0;
    border-bottom:#256fbd 1px solid;
}
.refill_options p{
    margin-bottom:0;
}
#medicationModalRefill .modal-dialog{
    top:inherit;
    width:100%;
    margin:0 0 0;
    padding:15px 0 0;
    -webkit-transform:none;
    transform: none;
}
#medicationModalRefill #textarea{
    width:100%;
    background:#f1f1f1;
    resize:none;
    padding:7px;
}
.InsuranceCardFrontModel{
    /*    padding-left: 19px;*/
    padding-top: 10px;
    padding-bottom: 8px;
}
.InsuranceCardFrontModel div{
    font-weight:bold;
    color: #666666;
}
.InsuranceBackCardModel{
    padding-top: 10px;
    padding-bottom: 8px;
    position: relative;
    /*    left: 93px;*/
}
.InsuranceBackCardModel div{
    font-weight:bold;
    color: #666666;
}
.frontsidecardimg1 {
    border: 1px solid solid;
    cursor: pointer;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    float:left;
    margin:0 0.5%;
}
.insurancecardfront{
    position:relative;
    left:20px;
}
.insurancecardback{
    position:relative;
    left:58px
}
.dependentmodal li{
    float: none;
    margin: 7px 0 0;
}
.dependentmodal .scroll{
    margin: 0;
    height: 35px;
}
.dependentmodal .modal-body{
    padding:10px !important;
    height: 360px;
}
.dependentmodal .modal-content label{
    color: #ffffff;
    font-weight: bold;
}
.dependentmodal ul{
    padding-left:0px !important;
}
.dependentmodal .video_div_two {
    border: #999999 5px solid;
    border-radius: 0;
    overflow: auto;
    height: 300px;
}
.dependentmodal .video_div_two .table-three thead{
    background: #2171b6;
    color: #ffffff;
    font-size: 12px;
    font-weight: normal;
    font-family: 'Roboto Condensed', sans-serif;
}
.dependentmodal .video_div_two .tableContainer tbody {
    font-size: 11px;
    color: #000000;
    font-weight: 600;
    text-transform: uppercase;
    border: 1px solid #d1ced3;
}
.dependentmodal .video_div_two .table-three tr:nth-child(2) {
    background: #e9e9e9;
    color: #000000;
}
.dependentmodal .video_div_two tr td:first-child {
    text-decoration: none;
    color: #000000;
}
.dependentmodal .video_div_two .table > thead > tr > th {
    vertical-align: bottom;
    border-bottom: none;
    border-right: 1px solid #ffffff;
    font-size: 9px;
    font-weight: 400px;
    min-width: 85px;
    color: #ffffff;
    padding-left: 5px;
}
.dependentmodal .modal-title span{
    left: 50%;
    position: fixed;
}
.dependentmodal li{
    width: 70%;
}
.dependentmodal.modal .modal-dialog, .dependentmodal.modal .modal-content{
    width: 1150px !important;
}
.dependentmodal .rx_lab h5{
    font-size: 14px;
}
