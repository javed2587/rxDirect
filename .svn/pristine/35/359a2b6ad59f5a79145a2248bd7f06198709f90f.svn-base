.float-label .control {

    position: relative;

}

.float-label input,

.float-label textarea {

    display: block;

    outline: 0;

    resize: none;

}

.float-label input + label,

.float-label textarea + label {

    position: absolute;

    top: 10px;

    transition: top 0.7s ease, opacity 0.7s ease;

    opacity: 0;

    font-size: 13px;

    font-weight: 600;

    color: #ccc;

}

.float-label input:valid + label,

.float-label textarea:valid + label {

    opacity: 1;

    top: 0px;

    left: 20px;

    margin: 0;

    outline: 0;

    color: #2368b2;

    font-size: 11px;

} 

.float-label .control input:valid{

    padding-top:10px;

    padding-bottom:2px;

}

.float-label input:focus + label,

.float-label textarea:focus + label { 

    color: #2c8efe;

}

.float-label .control.first_field input:valid + label,

.float-label .control.first_field textarea:valid + label {

    left: 27px;

}

.float-label .control.secondary_float input:valid + label,

.float-label .control.secondary_float textarea:valid + label {

    left: 13px;

}

