.drug_modals .radio{  

    position: relative;

}

.drug_modals .radio label{

    display:inline-block;

    color:#333333;

    font-size:14px;

    font-weight:normal;

    font-family: 'Roboto Condensed', sans-serif;

    margin-right:10px;

}

.drug_modals .radio input[type="radio"]{

    margin-top:3px;

    position: absolute;

    visibility: hidden;

}

.drug_modals .radio ul{

    list-style: none;

    margin: 0;

    padding: 0;

}

.drug_modals .radio ul li{

    display: block;

    position: relative;

    float: left;

}

.drug_modals .radio ul li input[type=radio]{

    position: absolute;

    visibility: hidden;

}

.drug_modals .radio ul li label{

    display: block;

    position: relative;

    font-weight: normal;

    color:#333333;

    font-size: 14px;

    line-height:23px;

    padding: 0px 0 0px 29px;

    z-index: 9;

    cursor: pointer;

    -webkit-transition: all 0.25s linear;

}

.drug_modals .radio ul li:hover label{

    color: #333333;

}

.drug_modals .radio ul li .check{

    display: block;

    position: absolute;

    border: 2px solid #676767;

    background:#FFF;

    border-radius: 100%;

    height: 23px;

    width: 23px;

    top: 0px;

    left: 0px;

    z-index: 5;

    transition: border .25s linear;

    -webkit-transition: border .25s linear;

}

.drug_modals .radio ul li:hover .check {

    border: 2px solid #676767;

}

.drug_modals .radio ul li .check::before {

    display: block;

    position: absolute;

    content: '';

    border-radius: 100%;

    height: 13px;

    width: 13px;

    top: 3px;

    left: 3px;

    margin: auto;

    transition: background 0.25s linear;

    -webkit-transition: background 0.25s linear;

}

.drug_modals input[type=radio]:checked ~ .check {

    border: 2px solid #7eb514;

}

.drug_modals input[type=radio]:checked ~ .check::before{

    background: #7eb514;

}

.drug_modals input[type=radio]:checked ~ label{

    color: #7eb514;

}

