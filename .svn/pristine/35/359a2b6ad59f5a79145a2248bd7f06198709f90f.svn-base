/*Custom Left*/

.cutome_left{

    position:relative;

}

.cutome_left .tque_left{

}

.cutome_left .custome_right{

    -moz-transition:all 200ms ease-in;

    -webkit-transition:all 200ms ease-in;

    -o-transition:all 200ms ease-in;

    transition:all 200ms ease-in;

    padding-left:27px;

}

.cutome_left .tque_left{

    position:absolute;

    top:0;

    left:0;

    margin:0 0 0 -250px;

    width:250px;

    -moz-transition:all 200ms ease-in;

    -webkit-transition:all 200ms ease-in;

    -o-transition:all 200ms ease-in;

    transition:all 200ms ease-in;

    z-index:100;

    padding-bottom:50px;

    opacity:0;

}

.cutome_left label.cutmobtn{

    display:block;

    font-family:'helvetica neue', helvetica, arial, sans-serif;

    font-weight:700;

    background:url(../../images/arrow-r.png) no-repeat;

    width:16px;

    height:32px;

    text-align:center;

    font-size:2em;

    line-height:1.1em;

    position:absolute;

    top:0;

    left:10px;

    text-decoration:none;

    -moz-transition:all 200ms ease-in;

    -webkit-transition:all 200ms ease-in;

    -o-transition:all 200ms ease-in;

    transition:all 200ms ease-in;

    z-index:500;

    cursor:pointer;

}

.cutome_left label.cutmobtn.btnopenedc{

    background:url(../../images/arrow-l.png) no-repeat;

}

.cutome_left input[type="checkbox"]{

    display:none;

}

.cutome_left input[type="checkbox"]:checked ~ .tque_left{

    margin:0;

    opacity:1;

}

.cutome_left input[type="checkbox"]:checked ~ label.cutmobtn{

    left:240px;

}

.cutome_left input[type="checkbox"]:checked ~ .custome_right{

    /*-webkit-transform:translate3d(260px, 0, 0);

    -moz-transform:translate3d(260px, 0, 0);

    -o-transform:translate3d(260px, 0, 0);

