    border: 1px solid #3984c7;

}

.checkbox_style .check::before {

    display: block;

    position: absolute;

    content: '';

    border-radius: 50%;

    height: 8px;

    width: 8px;

    top: 3px;

    left: 3px;

    margin: auto;

    transition: background 0.25s linear;

    -webkit-transition: background 0.25s linear;

}

.checkbox_style input[type=radio]:checked ~ .check {

    border: 1px solid #3984c7;

}

.checkbox_style input[type=radio]:checked ~ .check::before{

    background: #000000;

}

.checkbox_style input[type=radio]:checked ~ label{

    color: #000000;

}

.public_check{

}

.checkbox_style.public_check:hover .check{

    border: 1px solid #e01818;

}

.checkbox_style.public_check .check{

    border: 1px solid #e01818;

}

.checkbox_style.public_check label{

    color:#e01818;

}

/*Checkbox Style ends*/

